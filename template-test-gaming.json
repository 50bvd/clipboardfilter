{
    "filters":  [
                    {
                        "pattern":  "[0-9A-F]{32}",
                        "description":  "Steam API Key",
                        "useRegex":  true,
                        "replacement":  "STEAM_KEY_***REDACTED***",
                        "category":  "Gaming",
                        "enabled":  true
                    },
                    {
                        "pattern":  "[A-Za-z0-9_-]{24}\\.[A-Za-z0-9_-]{6}\\.[A-Za-z0-9_-]{27,}",
                        "description":  "Discord Bot Token",
                        "useRegex":  true,
                        "replacement":  "DISCORD_BOT_***REDACTED***",
                        "category":  "Gaming",
                        "enabled":  true
                    },
                    {
                        "pattern":  "oauth:[a-z0-9]{30}",
                        "description":  "Twitch OAuth Token",
                        "useRegex":  true,
                        "replacement":  "oauth:***REDACTED***",
                        "category":  "Gaming",
                        "enabled":  true
                    },
                    {
                        "pattern":  "[0-9a-f]{32}",
                        "description":  "Epic Games Exchange Code",
                        "useRegex":  true,
                        "replacement":  "EPIC_***REDACTED***",
                        "category":  "Gaming",
                        "enabled":  false
                    }
                ],
    "description":  "Template de test avec filtres gaming - Steam, Discord, Twitch",
    "name":  "Test Gaming Filters",
    "author":  "ClipboardFilter Team",
    "version":  "1.0.0"
}
